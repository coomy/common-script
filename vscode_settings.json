    "files.associations": {
        // Adds xml syntax highlighting for grd files.
        "*.grd" : "xml",
        // Optional: .gn and .gni are not JavaScript, but at least it gives some
        // approximate syntax highlighting. Ignore the linter warnings!
        "*.gni" : "javascript",
        "*.gn" : "javascript"
    },
    "files.watcherExclude": {
        // Don't watch out*/ and third_party/ for changes to fix an issue
        // where vscode doesn't notice that files have changed.
        // https://github.com/Microsoft/vscode/issues/3998
        // There is currently another issue that requires a leading **/ for
        // watcherExlude. Beware that this pattern might affect other out* folders
        // like src/cc/output/.
        "**/out*/**": true, 
        "**/.git": true,
        "**/third_party/**": true
    },
    "ycmd.path": "/home/coomy/.ycmd",
    "ycmd.debug": true,
    "ycmd.enabled_languages": [
        "cpp",
        "c",
        "cc",
        "h",
    ],
    "ycmd.confirm_extra_conf": false,
    "ycmd.use_imprecise_get_type": true,
    "ycmd.global_extra_config": 
      "/home/coomy/code/ttwebview/src/tools/vim/chromium.ycm_extra_conf.py",
    "C_Cpp.clang_format_fallbackStyle": "Chromium",
    // "C_Cpp.clang_format_path": "clang-format",
    "C_Cpp.clang_format_style": "Chromium",
    "C_Cpp.default.cppStandard": "c++11",
    "C_Cpp.clang_format_path": "/home/coomy/code/ttwebview/src/third_party/depot_tools/clang-format",
    "C_Cpp.clang_format_sortIncludes": false,
    "window.zoomLevel": 0,
    "editor.fontSize": 12,

